package com.epam.lab1.task2.back;

public class Calculation {

	public Calculation() {
	}
	
	private int getSumOfDivisors(int number){
		int sum = 0;
		for (int i = 0; i < number/2; i++) {
			if(number%i == 0){
				sum+=i;
			}
		}
		return sum;
	}
	
